<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
 	<class name="com.EntityClasses.Passenger" table="passenger_table">
 	 	
 	   <id name= "id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        
        <many-to-one name="destination_id" class="com.EntityClasses.Destination_Master" fetch="select">
            <column name="destination_id" not-null="true" />
        </many-to-one>
         <many-to-one name="user_id" class="com.EntityClasses.User_Master" fetch="select">
            <column name="user_id" not-null="true" />
        </many-to-one>
         <many-to-one name="bus_id" class="com.EntityClasses.Bus_Master" fetch="select">
            <column name="bus_id" not-null="true" />
        </many-to-one>
       
        
        <property name="date_of_booking" type="string">
            <column name="date_of_booking" not-null="true" />
        </property>
      
        <property name="date_of_traval" type="string">
            <column name="date_of_traval" not-null="true" />
        </property>
        <property name="seat_number" type="int">
            <column name="seat_number" not-null="true" />
        </property>
        <property name="ticket_qrcode" type="string">
            <column name="ticket_qrcode" not-null="true" />
        </property>
       
        
 	</class>
</hibernate-mapping>